{
    "compilerOptions": {
        "jsx": "react-jsx",
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "noEmit": true,
      "esModuleInterop": true,
      "isolatedModules": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  